<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="BrandedPrimaryColour">#FF1FFF89</Color>
    <Color x:Key="BrandedSecondaryColour">#FF5BA293</Color>
    <Color x:Key="BrandedSecondaryLightColour">#3DB0A4</Color>
    <Color x:Key="BrandedTertiaryLightestColour">#B7BAC0</Color>
    <Color x:Key="BrandedErrorColour">#FFFFFC00</Color>

    <SolidColorBrush x:Key="BrandedPrimaryColourBrush" Color="{StaticResource BrandedPrimaryColour}"/>
    <SolidColorBrush x:Key="BrandedSecondaryColourBrush" Color="{StaticResource BrandedSecondaryColour}"/>
    <SolidColorBrush x:Key="BrandedSecondaryLightColourBrush" Color="{StaticResource BrandedSecondaryLightColour}"/>
    <SolidColorBrush x:Key="BrandedTertiaryLightestColourBrush" Color="{StaticResource BrandedTertiaryLightestColour}"/>
    <SolidColorBrush x:Key="BrandedErrorColourBrush" Color="{StaticResource BrandedErrorColour}"/>
</ResourceDictionary>
